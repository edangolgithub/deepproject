{
    "version": "0.2.0",
    "configurations": [

       
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/dotnetlambda/test/DotNetLambda.Tests/bin/Debug/netcoreapp3.1/DotNetLambda.Tests.dll",
            "args": [],
            "cwd": "${workspaceFolder}/dotnetlambda/test/DotNetLambda.Tests",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}